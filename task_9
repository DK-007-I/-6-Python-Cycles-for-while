def main():
    while True:
        number = int(input("Введите число от 1 до 9:"))
        if 1 <= number <=9:
            break
        else:
            print("Неверный ввод! Введите числ от  1 до 9")




    print(f"\n Таблица умнажения для числа {number}: ")
    for i in range(1, 11):
        result = number * i
        print(f"{number} x {i} = {result}")

if __name__ == "__main__":
    main()
